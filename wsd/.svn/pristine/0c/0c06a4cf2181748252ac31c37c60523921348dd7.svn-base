/**
 * 首字母大写
 * @param {String} string
 */
export function uppercaseFirst(string) {
  return string.charAt(0).toUpperCase() + string.slice(1);
}
/**
 * 是否在线转换
 * @param {String} string 
 * @returns 
 */
export function isOnline(string) {
  return string === 'WLZT_ZX' ?  "在线":"离线";
}
/**
 * 格式化状态
 * @param {String} string 
 * @returns 
 */
export function formatState(string){
  switch(string){
    case'QY':
      return "启用";
    case'TY':
      return "停用";
    case'XZ':
      return "闲置";
    case'WX':
      return "维修";
  }
}
export function theformatState(string) {
  switch (string) {
    case '启用':
      return "QY";
    case '停用':
      return "TY";
    case '闲置':
      return "XZ";
    case '维修':
      return "WX";
  }
}
export function formatType(string) {
